<?xml version="1.0"?>
 
<!-- 28 Volt electrical system -->

<PropertyList>

  <!-- Supplier list -->

  
  <output>
    <name>Turn Coordinator Power</name>
    <prop>/systems/electrical/outputs/turn-coordinator</prop>
  </output>

  
  
  
  <!-- connect bus 2 outputs -->

